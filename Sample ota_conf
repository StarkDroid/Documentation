device_name=ro.product.device
#
# VERSION
#
# Example: Parse the 20150426 from the version name
#
# Version name: orion-falcon-5.1.1.release.1.3-OFFICIAL-20150731
# Version delimiter: -
# Version position: 3 (zero based)
# Version format: yyyyMMdd
#
# Note :
# * Version name must contain a parseable date
# * Version name must match with the Filename in the OTA XML file in order to compare the date
#
version_name=ro.modversion
